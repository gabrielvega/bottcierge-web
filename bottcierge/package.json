    "@angular/localize": "^18.1.3",
    "@angular/platform-browser": "^18.1.3",
    "@angular/platform-browser-dynamic": "^18.1.3",
    "@angular/router": "^18.1.3",
    "@byagasocial/auth": "^1.0.0",
    "@ngrx/effects": "^18.0.2",
    "@ngrx/entity": "^18.0.2",
    "@ngrx/operators": "^18.0.0",
    "@ngrx/store": "^18.0.2",
    "@ngrx/store-devtools": "^18.0.2",
    "bootstrap": "^5.3.0",
    "rxjs": "^7.8.0",
    "theme": "file:../../aga-social-auth/dist/theme",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.8"
