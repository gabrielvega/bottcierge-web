{
  "name": "bottcierge-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --localize && xliffmerge --profile xliffmerge.json",
    "build:pages": "ng build --output-path ../docs --base-href /bottcierge-web/",
